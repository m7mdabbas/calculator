class Calculator: 
    def __init__(self): 
        self.result = 0 
  
    # Add
    def add(self, a, b): 
        self.result = a + b 
        return self.result 
      
    # Subtract
    def subtract(self, a, b): 
        self.result = a - b 
        return self.result 
  
    # Multiply
    def multiply(self, a, b): 
        self.result = a * b 
        return self.result 
  
    # Divide
    def divide(self, a, b): 
        self.result = a / b 
        return self.result 
  
    # power
    def power(self, a, b): 
        self.result = a ** b 
        return self.result 
    
    # divideint
    def divideint(self, a, b):
        self.result = a // b
        return self.result
